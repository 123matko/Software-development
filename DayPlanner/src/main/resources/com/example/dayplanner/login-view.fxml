<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="com.example.dayplanner.LoginController"
            prefHeight="500.0" prefWidth="800.0">

    <TextField fx:id="username" alignment="CENTER" layoutX="229.0" layoutY="151.0" text="username"/>
    <TextField fx:id="password" alignment="CENTER" layoutX="229.0" layoutY="200.0" text="password"/>

    <Button fx:id="Login" layoutX="274.0" layoutY="315.0" onAction="#onLoginButtonClick" mnemonicParsing="false" text="Login"/>
</AnchorPane>
